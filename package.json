{
    "name": "@cspruit/array-like-map",
    "version": "1.2.4",
    "description": "Extends functionality of the map to be more like an array",
    "source": "./array-like-map.js",
    "main": "dist/array-like-map.js",
    "module": "dist/array-like-map.mjs",
    "unpkg": "dist/array-like-map.umd.js",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "npm run flush && microbundle",
        "lint": "xo",
        "test": "nyc --reporter=lcov ava",
        "flush": "rm -r ./dist || true",
        "semantic-release": "semantic-release"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CassandraSpruit/Array-Like-Map.git"
    },
    "keywords": [
        "library",
        "javascript",
        "arrays"
    ],
    "author": "Cassandra Spruit",
    "license": "GPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/CassandraSpruit/Array-Like-Map/issues"
    },
    "homepage": "https://github.com/CassandraSpruit/Array-Like-Map#readme",
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@babel/preset-env": "^7.8.3",
        "@semantic-release/changelog": "^5.0.0",
        "@semantic-release/git": "^9.0.0",
        "ava": "2.4.0",
        "codecov": "^3.6.1",
        "microbundle": "^0.11.0",
        "nyc": "^15.0.0",
        "semantic-release": "^17.0.0",
        "xo": "^0.27.0"
    },
    "xo": {
        "env": "browser",
        "rules": {
            "unicorn/prefer-spread": "off"
        }
    }
}
